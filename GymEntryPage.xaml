<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proiect1.GymEntryPage"
             Title="Gym Entry Page">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Add a gym" Clicked="OnGymInfoAddedClicked" />
    </ContentPage.ToolbarItems>
    <ListView x:Name="listView" Margin="20" ItemSelected="OnListViewItemSelected">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding GymName}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>