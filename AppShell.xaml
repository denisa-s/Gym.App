<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proiect1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proiect1"
    xmlns:views="clr-namespace:Proiect1.Views"
    xmlns:models="clr-namespace:Proiect1.Models">

    <TabBar>
        <ShellContent Title="Home Page" ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent Title="Borrowings" ContentTemplate="{DataTemplate views:Borrowings}" />
        <ShellContent Title="Categories" ContentTemplate="{DataTemplate views:Categories}" />
        <ShellContent Title="Clients" ContentTemplate="{DataTemplate views:Clients}" />
        <ShellContent Title="Gyms" ContentTemplate="{DataTemplate views:Gyms}" />
        <ShellContent Title="Trainers" ContentTemplate="{DataTemplate views:Trainers}" />
        <ShellContent Title="My gyms" ContentTemplate="{DataTemplate local:GymEntryPage}" />
    </TabBar>
</Shell>
