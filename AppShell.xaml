<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proiect1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proiect1"
    xmlns:views="clr-namespace:Proiect1.Views"
    xmlns:models="clr-namespace:Proiect1.Models"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent Title="Home Page" ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent Title="My gyms" ContentTemplate="{DataTemplate local:GymEntryPage}" />
        <ShellContent Title="My trainers" ContentTemplate="{DataTemplate local:TrainerEntryPage}" />
        <ShellContent Title="My clients" ContentTemplate="{DataTemplate local:ClientEntryPage}" />
        <ShellContent Title="My subscriptions" ContentTemplate="{DataTemplate local:SubscriptionEntryPage}" />
        <ShellContent Title="My categories" ContentTemplate="{DataTemplate local:CategoryEntryPage}" />
      
        
    </TabBar>
</Shell>
